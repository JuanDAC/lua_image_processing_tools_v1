RegExp ReplaceAll:
- "(\})(\n)" to "$1;$2";
- "(\s*)(=|~)(\s*)" to "$2";
- "(\S+)(\s*)(end{1,1})(;)" to "$1$2$3";
- "(\S+)(;)(\s*)(end{1,1})" to "$1$3$4";
- "(\s*)(\.{2,2})(\s*)" to "$2";
- "(\t+)(end)(\s*)(\n)" to "$1$2;$4";
